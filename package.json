{
  "name": "find-imports",
  "version": "0.5.2",
  "description": "Find all imported modules in JavaScript files.",
  "homepage": "https://github.com/cheton/find-imports",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run eslint && npm test",
    "eslint": "eslint *.js",
    "test": "tap test/*.js --coverage --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill",
    "coveralls": "tap test/*.js --coverage --coverage-report=text-lcov --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill | coveralls"
  },
  "author": "Cheton Wu <cheton@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:cheton/find-imports.git"
  },
  "keywords": [],
  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "esprima": "^3.1.3",
    "glob": "^7.1.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "coveralls": "^2.13.1",
    "eslint": "^3.17.1",
    "tap": "^10.3.0"
  },
  "nyc": {
    "exclude": [
      "test/index.js"
    ]
  }
}
